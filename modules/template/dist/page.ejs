<header class="main-header">
    <div class="container">
        <h1 class="page-title"><%= typeof title != "undefined" ? title : "" %></h1>

        <ol class="breadcrumb pull-right">
            <li><a href="/">Home</a></li>
            <% if(typeof section != "undefined") { %>
            <li><a href="<%= typeof sectionUrl != "undefined" ? sectionUrl : "" %>"><%= section %></a></li>
